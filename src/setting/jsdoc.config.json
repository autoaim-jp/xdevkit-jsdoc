{
  "source": {
    "include": ["service"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "node_modules"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "recurse": true,
    "destination": "./doc/jsdoc/build/"
  }
}

